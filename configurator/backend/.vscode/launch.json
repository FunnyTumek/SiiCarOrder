{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch configurator",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "bin/Debug/net7.0/Configurator.Api.dll",
      "args": [],
      "cwd": "${workspaceFolder}/Configurator.Api",
      "stopAtEntry": false,
      "serverReadyAction": {
        "action": "openExternally",
        "uriFormat": "%s/swagger/index.html",
        "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
      },
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    }
  ]
}
