{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "build",
      "command": "dotnet",
      "type": "process",
      "options": {
        "cwd": "${workspaceFolder}/Configurator.Api"
      },
      "args": [
        "build",
        "/property:GenerateFullPaths=true",
        "/consoleloggerparameters:NoSummary"
      ],
      "problemMatcher": "$msCompile"
    },
    {
      "label": "publish",
      "command": "dotnet",
      "type": "process",
      "options": {
        "cwd": "${workspaceFolder}/Configurator.Api"
      },
      "args": [
        "publish",
        "/property:GenerateFullPaths=true",
        "/consoleloggerparameters:NoSummary"
      ],
      "problemMatcher": "$msCompile"
    },
    {
      "label": "watch",
      "command": "dotnet",
      "type": "process",
      "options": {
        "cwd": "${workspaceFolder}/Configurator.Api"
      },
      "args": [
        "watch",
        "run",
        "--project",
        "Configurator.Api.csproj"
      ],
      "problemMatcher": "$msCompile"
    }
  ]
}